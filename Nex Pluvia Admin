BaseballBat.Name = "BaseballBat"
BaseballBat.Parent = ScrollingFrame
BaseballBat.BackgroundColor3 = Color3.fromRGB(250, 128, 114)
BaseballBat.Position = UDim2.new(0.00934579782, 0, 0.489737064, 0)
