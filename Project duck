settingsname.Size = UDim2.new(1, 0, 0, 40)
settingsname.Font = Enum.Font.SourceSans
settingsname.FontSize = Enum.FontSize.Size24
settingsname.Text = "Settings"
settingsname.TextColor3 = Color3.new(1, 1, 1)

local lftbtn = Instance.new("TextButton")
lftbtn.Name = "Left Button"
lftbtn.Parent = settings
lftbtn.BackgroundColor3 = Color3.new(0, 0, 0)
lftbtn.BorderColor3 = Color3.new(1, 0, 0)
lftbtn.BorderSizePixel = 3
lftbtn.Position = UDim2.new(0, 0, 0, 40)
lftbtn.Size = UDim2.new(0.5, 0, 0, 40)
lftbtn.Font = Enum.Font.SourceSans
lftbtn.FontSize = Enum.FontSize.Size48
lftbtn.Text = "<"
lftbtn.TextColor3 = Color3.new(1, 1, 1)

local Page1 = Instance.new("Frame") -- Assuming these pages exist and are parented within 'settings'
Page1.Name = "Page1"
Page1.Parent = settings
Page1.BackgroundColor3 = Color3.new(0, 0, 0)
Page1.BorderColor3 = Color3.new(1, 0, 0)
Page1.BorderSizePixel = 3
Page1.Position = UDim2.new(0, 0, 0, 83)
Page1.Size = UDim2.new(1, 0, 1, -83)

local Page2 = Instance.new("Frame")
Page2.Name = "Page2"
Page2.Parent = settings
Page2.BackgroundColor3 = Color3.new(0, 0, 0)
Page2.BorderColor3 = Color3.new(1, 0, 0)
Page2.BorderSizePixel = 3
Page2.Position = UDim2.new(0, 0, 0, 83)
Page2.Size = UDim2.new(1, 0, 1, -83)
Page2.Visible = false

local function settingsLeftButtonClick()
	if Page1.Visible then
		Page1.Visible = false
		Page2.Visible = true
	elseif Page2.Visible then
		Page2.Visible = false
		Page1.Visible = true
	end
end

lftbtn.MouseButton1Down:Connect(settingsLeftButtonClick)

local rghtbtn = Instance.new("TextButton")
rghtbtn.Name = "Right Button"
rghtbtn.Parent = settings
rghtbtn.BackgroundColor3 = Color3.new(0, 0, 0)
rghtbtn.BorderColor3 = Color3.new(1, 0, 0)
rghtbtn.BorderSizePixel = 3
rghtbtn.Position = UDim2.new(0.5, 3, 0, 40)
rghtbtn.Size = UDim2.new(0.5, -3, 0, 40)
rghtbtn.Font = Enum.Font.SourceSans
rghtbtn.FontSize = Enum.FontSize.Size48
rghtbtn.Text = ">"
rghtbtn.TextColor3 = Color3.new(1, 1, 1)

local function settingsRightButtonClick()
	if Page1.Visible then
		Page1.Visible = false
		Page2.Visible = true
	elseif Page2.Visible then
		Page2.Visible = false
		Page1.Visible = true
	end
end

rghtbtn.MouseButton1Down:Connect(settingsRightButtonClick)
