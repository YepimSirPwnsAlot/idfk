Baldi.Name = "Baldi"
Baldi.Parent = ScrollingFrame
Baldi.BackgroundColor3 = Color3.fromRGB(162, 162, 162)
Baldi.Position = UDim2.new(0.00934579782, 0, 0.85554719, 0)
Baldi.Size = UDim2.new(0, 419, 0, 36)
