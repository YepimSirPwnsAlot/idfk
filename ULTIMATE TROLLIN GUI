pants.PantsTemplate = "rbxassetid://144076759"

local wingspart = script.wingsaaa

for i,v in pairs(wingspart:GetChildren()) do
    if v:IsA("Attachment") then
        v.Parent = ch.Torso
    end
end

wingspart:Destroy()

local armstuff = script.ArmStuff

for i,v in pairs(armstuff:GetChildren()) do
    if v:IsA("Attachment") and v.Name == "RArm" then
        v.Parent = ch["Right Arm"]
    elseif v:IsA("Attachment") and v.Name == "LArm" then
        v.Parent = ch["Left Arm"]
    end
end

armstuff:Destroy()

