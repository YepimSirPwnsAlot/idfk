    function createsound(id,pitch,volume,parent)
        coroutine.resume(coroutine.create(function()
            local sound = Instance.new("Sound",parent)
            sound.SoundId = "rbxassetid://"..id
            sound.PlaybackSpeed = pitch
            sound.Volume = volume
            sound:Play()
            repeat
            game:GetService("RunService").Heartbeat:Wait()    
            until sound.Playing == false
            sound:Destroy()    
        end))
    end
    function KillerWAVE(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("Deep orange")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "http://www.roblox.com/asset/?id=169412804"
            for i = 0,3,0.3 do
                game:GetService("RunService").Heartbeat:Wait()
                wavemesh.Scale = wavemesh.Scale + Vector3.new(0.6,0.1,0.6)        
            end    
            wait(0.5)
            part:Destroy()    
        end))
    end
    local player = game.Players.LocalPlayer
    local mouse = player:GetMouse()
    -- GUN
    
    local banjo = script.Model
    local banjoweld = Instance.new("Weld",banjo.Gun)
    banjoweld.Part0 = banjo.Gun
    banjoweld.Part1 = ch["Right Arm"]
    
    function Killer()
        attack = true
        speed = 0
        jpower = 0
        chat("Have a taste of THIS.")
        wait(0)
        for i = 0,8,0.1 do
            game:GetService("RunService").Heartbeat:Wait()
            neck.C0 = neck.C0:Lerp(CFrame.new(0,1.5,0)*CFrame.Angles(math.rad(0),math.rad(-90),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,0,0)*CFrame.Angles(math.rad(0),math.rad(90),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0.5)*CFrame.Angles(math.rad(90),math.rad(0),math.rad(75)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-5)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)    
        end
        
        banjo.Gun.Transparency = 0
        banjo.Gun.Gun2.Transparency = 0
        banjo.Gun.die.Transparency = 0
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0.1)
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0.1)
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0.1)
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0.1)
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0.1)
        KillerWAVE(banjo.Gun.CFrame*CFrame.new(0,2,0),0)
        local Shoot = script.Shoot:Clone()
        local aaa = Instance.new("BodyVelocity")
        Shoot.Anchored = false
        Shoot.CFrame = ch["Right Arm"].CFrame
        Shoot.Parent = workspace
        aaa.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
        aaa.Velocity = mouse.Hit.p
        aaa.Parent = Shoot
        spawn(function()
            wait(0.2)
            Shoot.Touched:Connect(function(h)
                aaa.Velocity = Vector3.new(0,0,0)
                Shoot.BrickColor = BrickColor.new("White")
                if Shoot:FindFirstChild("Decal") then
                    Shoot.Decal:Destroy()
                end
                spawn(function()
                    for i = 1,50 do
                        Shoot.Transparency = Shoot.Transparency+0.05
                        wait(0.05)
                    end
                    Shoot:Destroy()
                end)
                if h.Parent ~= ch and h.Parent:FindFirstChildOfClass("Humanoid") then
                    h.Parent:BreakJoint()
                    h.Parent:FindFirstChild("Humanoid"):Destroy()
                end
            end)
        end)
        wait(0)
        banjo.Gun.Transparency = 1
        banjo.Gun.Gun2.Transparency = 1
        banjo.Gun.die.Transparency = 1
        attack = false
        speed = 75
        jpower = 75
    end
    
    function spawnlittlewave(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("White")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "rbxassetid://173770780"
            local a = 0
            for i = 0,5,0.3 do
                game:GetService("RunService").Heartbeat:Wait()
                a = a + 5
                part.CFrame = pos*CFrame.Angles(0,a,0)
                part.Transparency = part.Transparency + 0.025
                wavemesh.Scale = wavemesh.Scale + Vector3.new(0.6,0.6,0.6)        
            end    
            part:Destroy()    
        end))
    end
    
    function fastwave(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("White")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "rbxassetid://173770780"
            local a = 0
            for i = 0,5,0.3 do
                game:GetService("RunService").Heartbeat:Wait()
                a = a + 15
                part.CFrame = pos*CFrame.Angles(0,a,0)
                part.Transparency = part.Transparency + 0.07
                wavemesh.Scale = wavemesh.Scale + Vector3.new(0.6,0.25,0.6)        
            end    
            part:Destroy()    
        end))
    end
    
    function spawnwave(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("White")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "rbxassetid://173770780"
            local a = 0
            for i = 0,8,0.3 do
                game:GetService("RunService").Heartbeat:Wait()
                a = a + 5
                part.CFrame = pos*CFrame.Angles(0,a,0)
                part.Transparency = part.Transparency + 0.025
                wavemesh.Scale = wavemesh.Scale + Vector3.new(0.6,0.6,0.6)        
            end    
            part:Destroy()    
        end))
    end
    
    function spawnbiggerwavelol(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("White")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "rbxassetid://173770780"
            local a = 0
            for i = 0,40,0.1 do
                game:GetService("RunService").Heartbeat:Wait()
                a = a + 5
                part.CFrame = pos*CFrame.Angles(0,a,0)
                part.Transparency = part.Transparency + 0.025
                wavemesh.Scale = wavemesh.Scale + Vector3.new(2.25,0.95,2.25)        
            end    
            part:Destroy()    
        end))
    end
    
    function spawnbigwave(pos,trans)
        coroutine.resume(coroutine.create(function()
            local part = Instance.new("Part",workspace)
            part.Locked = true
            part.BrickColor = BrickColor.new("White")
            part.CanCollide = false
            part.Anchored = true
            part.Transparency = trans
            part.CFrame = pos
            local wavemesh = Instance.new("SpecialMesh",part)
            wavemesh.MeshId = "rbxassetid://173770780"
            local a = 0
            for i = 0,35,0.1 do
                game:GetService("RunService").Heartbeat:Wait()
                a = a + 5
                part.CFrame = pos*CFrame.Angles(0,a,0)
                part.Transparency = part.Transparency + 0.025
                wavemesh.Scale = wavemesh.Scale + Vector3.new(0.95,0.95,0.95)        
            end    
            part:Destroy()    
        end))
    end
    
    function DAEXPLOSION(pos)
        coroutine.resume(coroutine.create(function()
            local expart1 = Instance.new("Part",ch)
            expart1.Material = Enum.Material.Neon
            expart1.Anchored = true
            expart1.CanCollide = false
            expart1.Size = Vector3.new(7,7,7)
            expart1.BrickColor = BrickColor.new("Bright orange")
            local expart2 = Instance.new("Part",ch)
            expart2.Material = Enum.Material.Neon
            expart2.Anchored = true
            expart2.CanCollide = false
            expart2.Size = Vector3.new(7,7,7)
            expart2.BrickColor = BrickColor.new("CGA brown")
            expart1.Touched:Connect(function(hit)
                if hit.Parent ~= ch and hit.Parent:FindFirstChildOfClass("Humanoid") and hit.Parent:FindFirstChild("IsRaider") == nil then
                    hit.Parent:BreakJoints()
                end
            end)
            expart2.Touched:Connect(function(hit)
                if hit.Parent ~= ch and hit.Parent:FindFirstChildOfClass("Humanoid") and hit.Parent:FindFirstChild("IsRaider") == nil then
                    hit.Parent:BreakJoints()
                end
            end)
            repeat
            game:GetService("RunService").Heartbeat:Wait()
            expart1.CFrame = CFrame.new(pos)*CFrame.Angles(math.random(-750,750),math.random(-750,750),math.random(-750,750))
            expart2.CFrame = CFrame.new(pos)*CFrame.Angles(math.random(-750,750),math.random(-750,750),math.random(-750,750))
            expart1.Size = expart1.Size + Vector3.new(0.25,0.25,0.25)
            expart2.Size = expart2.Size + Vector3.new(0.25,0.25,0.25)
            expart1.Transparency = expart1.Transparency + 0.005
            expart2.Transparency = expart2.Transparency + 0.005
            until expart1.Transparency > 1
            expart1:Destroy()
            expart2:Destroy()
        end))
    end
    
    function chat(text)
        coroutine.resume(coroutine.create(function()
            if ch.Head:FindFirstChild("ChatStuff") == nil then
                local billgui = Instance.new("BillboardGui",ch.Head)
                billgui.Name = "ChatStuff"
                billgui.Size = UDim2.new(5,0,1,0)
                billgui.LightInfluence = 0
                billgui.StudsOffset = Vector3.new(0,1.45,0)
                local textlol = Instance.new("TextLabel",billgui)
                textlol.Name = "Text"
                textlol.BackgroundTransparency = 1
                textlol.Size = UDim2.new(1,0,1,0)
                textlol.Font = Enum.Font.Cartoon
                textlol.Text = text
                textlol.TextColor3 = Color3.new(1,1,1)
                textlol.TextStrokeColor3 = Color3.new(0,0,0)
                textlol.TextScaled = true
                textlol.TextStrokeTransparency = 0
                textlol.TextWrapped = true
                wait(1.5)
                for i = 0,4,0.1 do
                    game:GetService("RunService").Heartbeat:Wait()
                    billgui.StudsOffset = billgui.StudsOffset + Vector3.new(0,0.035,0)
                    textlol.TextTransparency = textlol.TextTransparency + 0.025
                    textlol.TextStrokeTransparency = textlol.TextStrokeTransparency + 0.025
                end
                billgui:Destroy()
            else
                repeat
                game:GetService("RunService").Heartbeat:Wait()
                until ch.Head:FindFirstChild("ChatStuff") == nil
            end
            return
        end))
    end
    
    function ripbacons()
        coroutine.resume(coroutine.create(function()
            createsound(427025525,1,5,root)
            for i,v in pairs(ring:GetChildren()) do
                if v:IsA("Part") and v.Name == "Soul" then
                    v.Transparency = 0
                    v:FindFirstChildOfClass("ParticleEmitter").Enabled = true
                end
            end
            for i,v in pairs(ring:GetChildren()) do
                if v:IsA("Model") and v.Name == "BaconHair" then
                    v:BreakJoints()
                    v:FindFirstChildOfClass("Humanoid").PlatformStand = true
                    for a,g in pairs(v:GetChildren()) do
                        if g:IsA("Part") then
                            local bodyvel = Instance.new("BodyVelocity",g)
                            bodyvel.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
                            bodyvel.Velocity = Vector3.new(0,5,0)
                            coroutine.wrap(function()
                                for lol = 0,8,0.1 do
                                    game:GetService("RunService").Heartbeat:Wait()
                                    g.Transparency = g.Transparency + 0.02
                                    v.Head.face.Transparency = v.Head.face.Transparency + 0.01
                                end
                            end)()
                        elseif g:IsA("Accessory") then
                            local bodyvel = Instance.new("BodyVelocity",g.Handle)
                            bodyvel.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
                            bodyvel.Velocity = Vector3.new(0,5,0)
                            coroutine.wrap(function()
                                for lol = 0,8,0.1 do
                                    game:GetService("RunService").Heartbeat:Wait()
                                    g.Handle.Transparency = g.Handle.Transparency + 0.02
                                end
                            end)()
                        end
                    end
                end
            end
            wait(3)
            nobacons = true
        end))
    end
    
    -- Attacks 
    
    function animtemplate()
        neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        torso.C0 = torso.C0:Lerp(CFrame.new(0,0,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        
    end
    
    function intro()
        attack = true
        root.Anchored = true
        --[[ch["Right Arm"].RArm:FindFirstChildOfClass("ParticleEmitter").Enabled = false
        ch["Left Arm"].LArm:FindFirstChildOfClass("ParticleEmitter").Enabled = false
        for i,v in pairs(ch.Torso:GetChildren()) do
            if v:IsA("Attachment") then
                for a,g in pairs(v:GetChildren()) do
                    if g:IsA("ParticleEmitter") then
                        g.Enabled = false
                    end
                end
            end
        end]]--
        for i = 0,4,0.1 do
            game:GetService("RunService").Heartbeat:Wait()
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,-1.15,0)*CFrame.Angles(math.rad(-35),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.75,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(5)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.75,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-5)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-0.85,0)*CFrame.Angles(math.rad(-25),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,0.25,-0.25)*CFrame.Angles(math.rad(35),math.rad(0),math.rad(0)),0.3)
        end
        wait(2)
        ripbacons()
        repeat
        game:GetService("RunService").Heartbeat:Wait()
        until nobacons == true
        local lolwhat = 0
        for i = 0,14,0.1 do
            game:GetService("RunService").Heartbeat:Wait()
            lolwhat = lolwhat + 0.01
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,-0.5)*CFrame.Angles(math.rad(25),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,10,0)*CFrame.Angles(math.rad(35),math.rad(0),math.rad(0)),0.2)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(5)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(-5)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-1,0)*CFrame.Angles(math.rad(-68),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(-68),math.rad(0),math.rad(0)),0.3)
        end
        wait(1)
        camerashake(6,8)
        createsound(2648563122,1,3,ch)
        local cylinderlol = Instance.new("Part",ch)
        cylinderlol.Anchored = true
        cylinderlol.CanCollide = false
        cylinderlol.Shape = Enum.PartType.Cylinder
        cylinderlol.BrickColor = BrickColor.new("White")
        cylinderlol.Material = Enum.Material.Neon
        cylinderlol.Size = Vector3.new(math.huge,0.1,0.1)
        cylinderlol.CFrame = root.CFrame*CFrame.Angles(0,0,math.rad(90))
        spawnbiggerwavelol(root.CFrame*CFrame.new(0,-15,0),0.2)
        coroutine.wrap(function()
            for i = 0,6,0.1 do
                game:GetService("RunService").Heartbeat:Wait()
                cylinderlol.Size = cylinderlol.Size:Lerp(Vector3.new(math.huge,45,45),0.3)
                cylinderlol.Transparency = cylinderlol.Transparency + 0.025
            end
            cylinderlol:Destroy()
        end)()
        ring.Parent = nil
        --root.CFrame = root.CFrame*CFrame.new(0,0,0)
        ch["Right Arm"].RArm:FindFirstChildOfClass("ParticleEmitter").Enabled = true
        ch["Left Arm"].LArm:FindFirstChildOfClass("ParticleEmitter").Enabled = true
        for i,v in pairs(ch.Torso:GetChildren()) do
            if v:IsA("Attachment") then
                for a,g in pairs(v:GetChildren()) do
                    if g:IsA("ParticleEmitter") then
                        g.Enabled = true
                    end
                end
            end
        end
        anim = "Idle"
        attack = false
        root.Anchored = false
    end
    
    function blacon()
        attack = true
        speed = 8
        baconblade.BaconBlade.Transparency = 0
        baconblade.BaconBlade.Trail.Enabled = true
        baconblade.BaconBlade.CanCollide = true
        for i = 0,2,0.1 do
            game:GetService("RunService").Heartbeat:Wait()
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(-15),math.rad(0),math.rad(-8)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,0.5+0.1*math.cos(sine/16),0)*CFrame.Angles(math.rad(-15),math.rad(90),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.25,0.8,0)*CFrame.Angles(math.rad(90),math.rad(90),math.rad(0)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-5)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.75,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(-10),math.rad(-10)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(10),math.rad(-5)),0.3)
        end
        createsound(4958427918,1,2,baconblade.Handle)
        touchlol = baconblade.BaconBlade.Touched:Connect(function(hit)
            if hit.Parent ~= ch and hit.Parent:FindFirstChildOfClass("Humanoid") then
                touchlol:Disconnect()
                camerashake(8,8)
                hit.Parent:BreakJoints()
                createsound(444667824,1,2,hit.Parent)
            end
        end)
        for i = 0,3,0.1 do
            game:GetService("RunService").Heartbeat:Wait()
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,0.5+0.1*math.cos(sine/16),0)*CFrame.Angles(math.rad(-15),math.rad(-25),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.25,0.8,0)*CFrame.Angles(math.rad(0),math.rad(90),math.rad(90))*CFrame.Angles(math.rad(-90),0,0),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-5)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.75,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(-10),math.rad(-10)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(10),math.rad(-5)),0.3)
        end
        touchlol:Disconnect()
        attack = false
        speed = 75
        baconblade.BaconBlade.Transparency = 1
        baconblade.BaconBlade.Trail.Enabled = false
        baconblade.BaconBlade.CanCollide = false
    end
    
    -- Humanoid Events
    
    hum.Running:Connect(function(spd)
        if spd > 0.1 then
            anim = "Walk"
        elseif spd < 0.1 then
            anim = "Idle"    
        end
    end)
    
    hum.StateChanged:Connect(function()
        if hum:GetState() == Enum.HumanoidStateType.Freefall then
            anim = "Jump"
            wait(0.5)
            anim = "Fall"
        end
    end)
    
    -- UserInput Stuff
    
    userinput.InputBegan:Connect(function(key)
        local k = key.KeyCode
        local k2 = key.UserInputType 
        if k2 == Enum.UserInputType.MouseButton1 and attack == false then
            blacon()
        end
        if k == Enum.KeyCode.N then
            Nove()
        end
        if k == Enum.KeyCode.Z then
            stomp()
        end
        if k == Enum.KeyCode.X then
            flakey()
        end
        if k == Enum.KeyCode.C then
            Killer()
        end
        if k == Enum.KeyCode.F then
            taunt()
        end
        if k == Enum.KeyCode.V then
            KarmaGotYouXD()
        end
        if k == Enum.KeyCode.M and canplay == true and canplaying == true then
            music:Pause()
            canplay = false
        elseif k == Enum.KeyCode.M and canplay == false and canplaying == true then
            music:Resume()
            canplay = true
        end
    end)
    
    userinput.InputEnded:Connect(function(key)
        local k = key.KeyCode
        local k2 = key.UserInputType 
    end)
    
    -- Animation
    
    intro()
    while true do
        game:GetService("RunService").Heartbeat:Wait()
        sine = sine + change
        hum.Health = math.huge
        hum.MaxHealth = math.huge
        hum.WalkSpeed = speed
        hum.JumpPower = jpower
        if anim == "Jump" and attack == false then
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(5),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,0,0)*CFrame.Angles(math.rad(10),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(15)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-15)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        end
        if anim == "Fall" and attack == false then
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(-15),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,0,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(60)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-60)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
        end
        if anim == "Idle" and attack == false then
            --[[neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(-15),math.rad(5),math.rad(-8)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,3+0.1*math.cos(sine/16),0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(120),math.rad(-15),math.rad(5)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(25),math.rad(-15),math.rad(0)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.75,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(-10),math.rad(-10)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(10),math.rad(-5)),0.3)
            ]]
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,0)*CFrame.Angles(math.rad(-15),math.rad(5),math.rad(-8)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,3+0.1*math.cos(sine/16),0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.3,0.5,0.5)*CFrame.Angles(math.rad(75),math.rad(0),math.rad(75)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.3,0.5,0.5)*CFrame.Angles(math.rad(75),math.rad(0),math.rad(-75)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.75,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(-10),math.rad(-10)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(10),math.rad(-5)),0.3)
            
        end
        if anim == "Walk" and attack == false then
            neck.C0 = neck.C0:Lerp(CFrame.new(0,0.5,-0.25)*CFrame.Angles(math.rad(35),math.rad(0),math.rad(0)),0.3)
            torso.C0 = torso.C0:Lerp(CFrame.new(0,1.5+0.1*math.cos(sine/16),0)*CFrame.Angles(math.rad(-90),math.rad(0),math.rad(0)),0.3)
            rs.C0 = rs.C0:Lerp(CFrame.new(1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(15)),0.3)
            ls.C0 = ls.C0:Lerp(CFrame.new(-1.5,0.5,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(-15)),0.3)
            rh.C0 = rh.C0:Lerp(CFrame.new(0.5,-1,0)*CFrame.Angles(math.rad(0),math.rad(0),math.rad(0)),0.3)
            lh.C0 = lh.C0:Lerp(CFrame.new(-0.5,-0.85,-0.5)*CFrame.Angles(math.rad(-25),math.rad(0),math.rad(0)),0.3)
        end
    end
